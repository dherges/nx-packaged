    "@nrwl/schematics": "6.0.2",
    "@types/jasmine": "~2.8.0",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~10.1.0",
    "codelyzer": "~4.3.0",
    "jasmine-core": "~3.1.0",
    "jasmine-marbles": "^0.3.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^1.0.0",
    "ng-packagr": "3.0.0",
    "prettier": "1.12.1",
    "protractor": "~5.3.0",
    "ts-node": "~6.0.0",
    "tsickle": "^0.29.0",
