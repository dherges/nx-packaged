    "@angular/language-service": "7.0.0-beta.3",
    "@nrwl/schematics": "6.2.1",
    "@types/jasmine": "~2.8.0",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~10.1.0",
    "codelyzer": "~4.4.0",
    "jasmine-core": "~3.2.0",
    "jasmine-marbles": "^0.3.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^1.0.0",
    "ng-packagr": "4.1.1",
    "prettier": "1.14.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
