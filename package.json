    "@angular/platform-browser-dynamic": "7.0.0-beta.3",
    "@angular/router": "7.0.0-beta.3",
    "@ngrx/effects": "6.1.0",
    "@ngrx/router-store": "6.1.0",
    "@ngrx/store": "6.1.0",
    "@ngrx/store-devtools": "6.1.0",
    "@nrwl/nx": "6.2.1",
    "core-js": "^2.4.1",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.19"
