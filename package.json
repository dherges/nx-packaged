    "@angular/router": "6.0.3",
    "@ngrx/effects": "6.0.1",
    "@ngrx/router-store": "6.0.1",
    "@ngrx/store": "6.0.1",
    "@ngrx/store-devtools": "6.0.1",
    "@nrwl/nx": "6.0.2",
    "core-js": "^2.4.1",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.1",
